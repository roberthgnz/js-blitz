{
  "name": "js-blitz",
  "productName": "js-blitz",
  "version": "0.0.7",
  "description": "Run JavaScript code in a flash",
  "main": ".vite/build/main.js",
  "scripts": {
    "start": "electron-forge start",
    "dev": "electron-forge start -- --dev",
    "package": "rimraf out && electron-forge package",
    "make": "rimraf out && electron-forge make",
    "publish": "dotenv -e .env electron-forge publish",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
